{
  "// Component": "Pagination - Page navigation with items per page and total count",
  "_meta": {
    "component": "pagination",
    "type": "molecule",
    "version": "2.0.0",
    "created": "2025-01-15"
  },
  "paginations": [
    {
      "name": "blogPagination",
      "itemsPerPage": {
        "selectName": "demoSelect1"
      },
      "navigationPager": {
        "name": "navPager",
        "siblingCount": 2,
        "icons": {
          "first": "demoBack1",
          "prev": "demoLeft1",
          "dots": "demoDots1",
          "next": "demoRight1",
          "last": "demoForward1"
        }
      },
      "totalCount": {
        "show": true
      }
    }
  ]
}