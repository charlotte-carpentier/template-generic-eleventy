{
  "globalStyle": "text-sm pagination",
  "variants": {
    "default": "text-gray-800 hover:text-gray-600",
    "primary": "text-blue-600 hover:text-blue-800 font-bold",
    "secondary": "text-gray-500 hover:text-gray-700"
  },
  "pager": {
    "name": "pager",
    "text": "Pager",
    "style": "default",
    "select": {
      "itemsPerPage": {
        "ref": "selects.item-count"
      }
    },
    "showTotalCount": true
  },
  "navigationPager": {
    "name": "navigation-pager",
    "text": "Navigation Pager",
    "children": [
      { "icon": "first", "url": "/first", "ref": "buttons.inventory" },
      { "icon": "prev", "url": "/prev", "ref": "buttons.inventory" },
      { "label": "1", "url": "/page/1", "current": true, "ref": "buttons.quantity_button" },
      { "label": "2", "url": "/page/2", "ref": "buttons.quantity_button" },
      { "label": "3", "url": "/page/3", "ref": "buttons.quantity_button" },
      { "label": "...", "url": "#", "ref": "buttons.quantity_button" },
      { "label": "6", "url": "/page/6", "ref": "buttons.quantity_button" },
      { "icon": "next", "url": "/next", "ref": "buttons.inventory" },
      { "icon": "last", "url": "/last", "ref": "buttons.inventory" }
    ]
  }
}
