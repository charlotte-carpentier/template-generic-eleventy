{
  "// Component": "Slider - Single value and range sliders with handle indicators",
  
  "_meta": {
    "component": "slider",
    "type": "molecule",
    "version": "1.0.0",
    "created": "2025-01-15"
  },
  
  "sliders": [
    {
      "name": "demoSlider1",
      "type": "single",
      "label": "Experience",
      "min": 0,
      "max": 20,
      "step": 1,
      "values": [8],
      "disabled": false,
      "handleIndicator": {
        "show": true,
        "variant": "outline"
      }
    },
    {
      "name": "demoRange1", 
      "type": "range",
      "label": "Range",
      "min": 0,
      "max": 20,
      "step": 1,
      "values": [8, 18],
      "disabled": false,
      "handleIndicator": {
        "show": true,
        "variant": "solid"
      }
    }
  ]
}